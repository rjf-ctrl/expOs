alias functionNum R1;
alias currentPID R2;


//check if function num is 3
if (functionNum != 3) then
	return;
endif;


currentPID = [SYSTEM_STATUS_TABLE +1];

multipush(R1, R2, R3);
R1 = 8;
R2 = currentPID;
call MOD_0; //to acquire terminal
//print "hi_mod0";
multipop(R1,R2, R3);
print (R3);
multipush(R1,R2,R3);
R1 = 9;
call MOD_0; //to release terminal
//print "bye_mod0";
multipop(R1,R2,R3);
return;

